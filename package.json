{
  "name": "@prokodo/cli",
  "version": "0.1.11",
  "description": "prokodo developer CLI â€” verify, inspect and manage your prokodo related services",
  "license": "MIT",
  "author": {
    "name": "prokodo",
    "email": "info@prokodo.com",
    "url": "https://www.prokodo.com/?utm_source=npm&utm_medium=author&utm_campaign=prokodo_cli"
  },
  "keywords": [
    "prokodo",
    "cli",
    "verify",
    "developer-tools",
    "devtools",
    "deployment",
    "ci"
  ],
  "type": "commonjs",
  "main": "dist/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/prokodo-agency/cli"
  },
  "homepage": "https://docs.cli.prokodo.com",
  "bugs": {
    "url": "https://github.com/prokodo-agency/cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "prokodo": "dist/cli.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "pnpm test:coverage && pnpm test:integration",
    "lint": "pnpm lint:tsc && pnpm lint:eslint",
    "lint:fix": "prettier --write . && eslint src --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf dist coverage",
    "prepack": "pnpm build",
    "docs:start": "cd public && pnpm start",
    "docs:build": "cd public && pnpm build",
    "docs:serve": "cd public && pnpm serve",
    "docs:typecheck": "cd public && pnpm typecheck",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.0.0",
    "commander": "^14.0.3",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
